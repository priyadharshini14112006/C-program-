#include <stdio.h>

int main() {

    int n, num;

    int positive = 0, negative = 0;

    // Read the number of elements

    scanf("%d", &n);

    // Read each number and count positives and negatives

    for (int i = 0; i < n; i++) {

        scanf("%d", &num);

        if (num > 0) {

            positive++;

        } else if (num < 0) {

            negative++;

        }

    }

    // Print counts as float

    printf("Count of Positive Integer is %.2f\n", (float)positive);

    printf("Count of Negative Integer is %.2f\n", (float)negative);

    return 0;

}
