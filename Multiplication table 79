include <stdio.h>

int main() {

    int n;

    // Read input

    scanf("%d", &n);

    // Check if input is valid (1 <= n <= 9)

    if (n >= 1 && n <= 9) {

        // Print multiplication table till n

        for (int i = 1; i <= n; i++) {

            printf("%d x %d = %d\n", n, i, n * i);

        }

    } else {

        printf("Invalid Input\n");

    }

    return 0;

}
