#include <stdio.h>

int main() {

    int num, digit1, digit2, sum;

    // Input from user

    scanf("%d", &num);

    // Split digits

    digit1 = num / 10;

    digit2 = num % 10;

    // Sum of digits

    sum = digit1 + digit2;

    // Output

    printf("Bird said:%d\n", num);

    printf("Alice must go in path-%d\n", sum);

    return 0;

}
