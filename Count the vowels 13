#include <stdio.h>

#include <ctype.h>

int countVowels(const char *str) {

    int count = 0;

    for (int i = 0; str[i] != '\0'; i++) {

        char ch = tolower(str[i]); 

        if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {

            count++;

        }

    }

    return count;

}

int main() {

    char word[100];

    

    

    scanf("%s", word);

    

    

    int result = countVowels(word);

    printf("count: %d\n", result);

    

    return 0;

}
